{
  "plugins": [],
  "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": "node_modules/|docs"
  },
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "JSON to CSV conversor",
      "meta" : [
        {
          "name": "author",
          "content": "Francisco Solis"
        },
        {
          "name": "description",
          "content": "A JSON to CSV converter with HTML, CSS and JavaScript using a vanilla router"
        }
      ]
     },
     "readme": "./README.md"
  },
  "template": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}

{
  "plugins": [],
  "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": "node_modules/|docs"
  },
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "JSON to CSV conversor"
     }
  },
  "template": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
